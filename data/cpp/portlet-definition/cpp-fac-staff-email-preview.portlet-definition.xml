<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portlet-definition version="4.0" xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition https://source.jasig.org/schemas/uportal/io/portlet-definition/portlet-definition-4.0.xsd" xmlns:ns2="https://source.jasig.org/schemas/uportal" xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" xmlns:ns4="https://source.jasig.org/schemas/uportal/io/subscribed-fragment" xmlns:ns3="https://source.jasig.org/schemas/uportal/io/stylesheet-descriptor" xmlns:ns5="https://source.jasig.org/schemas/uportal/io/event-aggregation" xmlns:ns6="https://source.jasig.org/schemas/uportal/io/user" xmlns:ns7="https://source.jasig.org/schemas/uportal/io/portlet-type" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ns8="https://source.jasig.org/schemas/uportal/io/permission-owner">
    <title>Faculty &amp; Staff Email Inbox Preview</title>
    <name>Faculty &amp; Staff Email Inbox Preview</name>
    <fname>cpp-fac-staff-email-preview</fname>
    <desc>Faculty &amp; Staff Email Access</desc>
    <type>Portlet</type>
    <timeout>5000</timeout>
    <portlet-descriptor>
        <ns2:webAppName>/email-preview</ns2:webAppName>
        <ns2:portletName>email-preview</ns2:portletName>
    </portlet-descriptor>
    <category>All categories</category>
    <group>Employees</group>
    <parameter>
        <name>alternate</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>blockImpersonation</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>configurable</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>disableDynamicTitle</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>editable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasAbout</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>hideFromMobile</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>highlight</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>printable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>showChrome</name>
        <value>true</value>
    </parameter>
    <portlet-preference>
        <name>PortletPreferencesCredentialsAuthenticationService.ACCOUNT_NAME_ATTRIBUTE</name>
        <readOnly>false</readOnly>
        <value></value>
    </portlet-preference>
    <portlet-preference>
        <name>authenticationServiceKey</name>
        <readOnly>false</readOnly>
        <value>portletPreferences</value>
    </portlet-preference>
    <portlet-preference>
        <name>connectionTimeout</name>
        <readOnly>false</readOnly>
        <value>25000</value>
    </portlet-preference>
    <portlet-preference>
        <name>exchangeAutodiscover</name>
        <readOnly>false</readOnly>
        <value>false</value>
    </portlet-preference>
    <portlet-preference>
        <name>exchangeUserDomain</name>
        <readOnly>false</readOnly>
        <value>win.csupomona.edu</value>
    </portlet-preference>
    <portlet-preference>
        <name>host</name>
        <readOnly>false</readOnly>
        <value>exchange-legacy.csupomona.edu</value>
    </portlet-preference>
    <portlet-preference>
        <name>inboxName</name>
        <readOnly>false</readOnly>
        <value>Inbox</value>
    </portlet-preference>
    <portlet-preference>
        <name>inboxUrl</name>
        <readOnly>false</readOnly>
        <value>https://exchange.csupomona.edu/owa/</value>
    </portlet-preference>
    <portlet-preference>
        <name>linkServiceKey</name>
        <readOnly>false</readOnly>
        <value>default</value>
    </portlet-preference>
    <portlet-preference>
        <name>port</name>
        <readOnly>false</readOnly>
        <value>-1</value>
    </portlet-preference>
    <portlet-preference>
        <name>protocol</name>
        <readOnly>false</readOnly>
        <value>ExchangeWebServices</value>
    </portlet-preference>
    <portlet-preference>
        <name>timeout</name>
        <readOnly>false</readOnly>
        <value>25000</value>
    </portlet-preference>
    <portlet-preference>
        <name>usernameSuffix</name>
        <readOnly>false</readOnly>
        <value>@csupomona.edu</value>
    </portlet-preference>
    <portlet-preference>
        <name>welcomeTitle</name>
        <readOnly>false</readOnly>
        <value>Mail</value>
    </portlet-preference>
</portlet-definition>
