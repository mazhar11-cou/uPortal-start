<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portlet-definition version="5.0" xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition https://source.jasig.org/schemas/uportal/io/portlet-definition/portlet-definition-5.0.xsd" xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" xmlns:ns6="https://source.jasig.org/schemas/uportal/io/permission-owner" xmlns:ns5="https://source.jasig.org/schemas/uportal/io/subscribed-fragment" xmlns:ns8="https://source.jasig.org/schemas/uportal/io/user" xmlns:ns7="https://source.jasig.org/schemas/uportal/io/stylesheet-descriptor" xmlns:ns2="https://source.jasig.org/schemas/uportal" xmlns:ns4="https://source.jasig.org/schemas/uportal/io/event-aggregation" xmlns:ns3="https://source.jasig.org/schemas/uportal/io/portlet-type" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <title>University Catalog</title>
    <name>University Catalog</name>
    <fname>university-catalog</fname>
    <desc>Explore our University catalog.</desc>
    <type>CMS</type>
    <timeout>5000</timeout>
    <portlet-descriptor>
        <ns2:webAppName>/SimpleContentPortlet</ns2:webAppName>
        <ns2:portletName>cms</ns2:portletName>
    </portlet-descriptor>
    <category>Favorites Starter Pack</category>
    <category>Recommended</category>
    <category>Online Services</category>
    <group>Students</group>
    <permissions>
        <permission system="UP_PORTLET_SUBSCRIBE" activity="BROWSE">
            <group>Students</group>
        </permission>
    </permissions>
    <parameter>
        <name>alternativeMaximizedLink</name>
        <value>//catalog.cpp.edu</value>
    </parameter>
    <parameter>
        <name>blockImpersonation</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>chromeStyle</name>
        <value>default</value>
    </parameter>
    <parameter>
        <name>configurable</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>disableDynamicTitle</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>disablePortletEvents</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>editable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasAbout</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hideFromMobile</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>iconUrl</name>
        <value>//www.cpp.edu/~mycpp/img/18advising_bac5-resize.jpg</value>
    </parameter>
    <parameter>
        <name>printable</name>
        <value>false</value>
    </parameter>
</portlet-definition>
