<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<portlet-definition xmlns="https://source.jasig.org/schemas/uportal/io/portlet-definition" xmlns:ns2="https://source.jasig.org/schemas/uportal" xmlns:ns3="https://source.jasig.org/schemas/uportal/io/event-aggregation" xmlns:ns4="https://source.jasig.org/schemas/uportal/io/permission-owner" xmlns:ns5="https://source.jasig.org/schemas/uportal/io/stylesheet-descriptor" xmlns:ns6="https://source.jasig.org/schemas/uportal/io/subscribed-fragment" xmlns:ns7="https://source.jasig.org/schemas/uportal/io/portlet-type" xmlns:ns8="https://source.jasig.org/schemas/uportal/io/user" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="5.0" xsi:schemaLocation="https://source.jasig.org/schemas/uportal/io/portlet-definition https://source.jasig.org/schemas/uportal/io/portlet-definition/portlet-definition-5.0.xsd">
    <title>BroncoDirect Student Center</title>
    <name>BroncoDirect Student Center</name>
    <fname>broncodirect-student-center</fname>
    <desc>Access all of the features related to student enrollment, fee payment, financial aid, and application status.</desc>
    <type>CMS</type>
    <timeout>5000</timeout>
    <portlet-descriptor>
        <ns2:webAppName>/SimpleContentPortlet</ns2:webAppName>
        <ns2:portletName>cms</ns2:portletName>
    </portlet-descriptor>
    <category>Favorites Starter Pack</category>
    <category>Online Services</category>
    <category>Quicklinks</category>
    <group>Applicants</group>
    <group>Pending Students</group>
    <group>Students</group>
    <group>Past Students</group>
    <permissions>
        <permission system="UP_PORTLET_SUBSCRIBE" activity="BROWSE">
            <group>Applicants</group>
            <group>Pending Students</group>
            <group>Students</group>
            <group>Past Students</group>
        </permission>
    </permissions>
    <parameter>
        <name>alternativeMaximizedLink</name>
        <value><![CDATA[https://idp.cpp.edu/idp/profile/cas/login?service=https://cmsweb.cms.cpp.edu/psp/CPOMPRDF/EMPLOYEE/SA/c/SA_LEARNER_SERVICES.SSS_STUDENT_CENTER.GBL?gsmobile=0&1=1]]></value>
    </parameter>
    <parameter>
        <name>blockImpersonation</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>chromeStyle</name>
        <value>default</value>
    </parameter>
    <parameter>
        <name>configurable</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>disableDynamicTitle</name>
        <value>true</value>
    </parameter>
    <parameter>
        <name>disablePortletEvents</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>editable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasAbout</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hasHelp</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>hideFromMobile</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>iconUrl</name>
        <value>//www.cpp.edu/mycpp/img/student_center-16x9.png</value>
    </parameter>
    <parameter>
        <name>printable</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>waffleIconUrl</name>
        <value>//www.cpp.edu/mycpp/img/student_center-square.png</value>
    </parameter>
    <parameter>
        <name>waffleLabel</name>
        <value>Student Center</value>
    </parameter>
</portlet-definition>
