        <value>false</value>
    </parameter>
    <parameter>
        <name>hideFromMobile</name>
        <value>false</value>
    </parameter>
    <parameter>
        <name>iconUrl</name>
        <value>//www.cpp.edu/~it_apps_assets/img/500/student_center-square.png</value>
    </parameter>
    <parameter>
        <name>printable</name>
