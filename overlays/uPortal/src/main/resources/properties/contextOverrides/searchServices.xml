<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.1.xsd">

    <util:list id="searchServices">
        <bean class="edu.cpp.campusapps.SearchServices.cludo.CludoSearchService">
            <property name="baseUrl" value="${edu.cpp.campusapps.SearchServices.cludo.CludoSearchService.baseUrl}"/>
            <property name="customerId" value="${edu.cpp.campusapps.SearchServices.cludo.CludoSearchService.customerId}"/>
            <property name="engineId" value="${edu.cpp.campusapps.SearchServices.cludo.CludoSearchService.engineId}"/>
            <property name="apiKey" value="${edu.cpp.campusapps.SearchServices.cludo.CludoSearchService.apiKey}"/>
        </bean>

        <bean class="edu.cpp.campusapps.SearchServices.PortletRegistry.CustomPortletRegistrySearchService">
            <property name="excludedCategories">
	        <list/>
            </property>
        </bean>
    </util:list>

</beans>
